{
  "name": "electron-server-angular",
  "version": "0.0.1",
  "description": "Scaffolding project to run a desktop applications with it's own server and AngularJS",
  "main": "main.js",
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "package": "./node_modules/.bin/electron-packager . app --overwrite --out dist",
    "setup": "./node_modules/.bin/electron-installer-windows --src ./dist/app-win32-x64/ --dest ./dist/installers/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcorral/electron-server-angular.git"
  },
  "keywords": [
    "electron",
    "angular",
    "server"
  ],
  "author": "Tomas Corral Casas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tcorral/electron-server-angular/issues"
  },
  "homepage": "https://github.com/tcorral/electron-server-angular#readme",
  "dependencies": {
    "electron": "^1.6.11",
    "mime": "^1.3.6",
    "portscanner": "^2.1.1"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.7.2"
  }
}
